#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);   // number of test cases

    while (T--) {
        int X, Y;
        scanf("%d %d", &X, &Y);   // dice values

        if (X + Y > 6) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}
